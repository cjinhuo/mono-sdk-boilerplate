{
  "name": "@mono/core",
  "version": "0.0.1",
  "description": "@mono/core",
  "author": "cjinhuo",
  "license": "MIT",
  "main": "src/index.ts",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "publishConfig": {
    "main": "dist/index.js"
  },
  "type": "module",
  "files": ["dist"],
  "scripts": {
    "dev": "run-s esm:watch build:watch",
    "esm": "tsc -b ./tsconfig.json",
    "build": "batch-rollup -d ./rollups",
    "esm:watch": "tsc -b -w ./tsconfig.json",
    "build:watch": "batch-rollup -d ./rollups -f cjs",
    "lint": "eslint ./src/**",
    "test": "jest --config jest.config.cjs",
    "help": "rollup -h"
  },
  "dependencies": {
    "@mono/rollup": "workspace:*",
    "@mono/shared": "workspace:*"
  },
  "devDependencies": {
    "@mono/tsconfig": "workspace:*",
    "@mono/eslint-config": "workspace:*"
  }
}
