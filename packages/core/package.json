{
  "name": "@mono/core",
  "version": "0.0.1",
  "description": "@mono/core",
  "author": "cjinhuo",
  "license": "MIT",
  "main": "src/index.ts",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "publishConfig": {
    "main": "dist/index.js"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "run-s esm:watch build:watch",
    "esm": "tsc -b ./tsconfig.json",
    "esm:watch": "tsc -b -w ./tsconfig.json",
    "build": "rollup -c rollup.config.js",
    "build:cjs": "rollup -c ./rollups/iife.rollup.mjs",
    "build:watch": "rollup -c  ./rollups/cjs.rollup.mjs --watch",
    "build:ts": "rollup -c rollup.config.ts --bundleConfigAsCjs --configPlugin typescript",
    "rollup-run": "rollup-run -f ./rollups",
    "help": "rollup -h"
  },
  "dependencies": {
    "@mono/rollup": "workspace:*",
    "@mono/shared": "workspace:*",
    "@mono/tsconfig": "workspace:*"
  }
}